{
    "name": "sepiphy/testing",
    "description": "The \"sepiphy/testing\" package.",
    "keywords": ["sepiphy", "tools", "phptools", "testing"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/phptools",
    "support": {
        "issues": "https://github.com/sepiphy/phptools/issues",
        "source": "https://github.com/sepiphy/phptools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan": "^0.11.4",
        "phpunit/phpunit": "^8.3",
        "sensiolabs/security-checker": "^6.0",
        "symfony/var-dumper": "^4.3",
        "xuanquynh/php-codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Testing\\": "./"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
