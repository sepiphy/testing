{
    "name": "sepiphy/testing",
    "description": "A proxy that requires a few popular testing libraries",
    "keywords": ["sepiphy", "tools", "phptools", "testing"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/tools",
    "support": {
        "issues": "https://github.com/sepiphy/tools/issues",
        "source": "https://github.com/sepiphy/tools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan": "^0.12.19",
        "phpunit/phpunit": "^8.3",
        "sensiolabs/security-checker": "^6.0",
        "symfony/var-dumper": "^4.4 | ^5.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
